<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:topLeftRadius="2.0dip" android:topRightRadius="2.0dip" android:bottomLeftRadius="2.0dip" android:bottomRightRadius="2.0dip" />
            <gradient android:startColor="@color/progress_grey" android:endColor="@color/progress_grey" android:centerColor="@color/progress_grey" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:topLeftRadius="2.0dip" android:topRightRadius="2.0dip" android:bottomLeftRadius="2.0dip" android:bottomRightRadius="2.0dip" />
                <gradient android:startColor="@color/progress_yellow" android:endColor="@color/progress_yellow" android:centerColor="@color/progress_yellow" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:topLeftRadius="2.0dip" android:topRightRadius="2.0dip" android:bottomLeftRadius="2.0dip" android:bottomRightRadius="2.0dip" />
                <gradient android:startColor="@color/progress_yellow" android:endColor="@color/progress_yellow" android:centerColor="@color/progress_yellow" />
            </shape>
        </clip>
    </item>
</layer-list>